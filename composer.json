{
    "name": "su-sws/stanford-caravan",
    "description": "Tools for testing Drupal 8 on CI/CD",
    "type": "library",
    "authors": [
        {
            "name": "Mike Decker",
            "email": "pookmish@stanford.edu"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:pookmish/behat-tools.git"
        }
    ],
    "require": {
        "boedah/robo-drush": "^4.2",
        "miiimooo/behat-tools": "dev-master",
        "behat/mink": "^1.7.0",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/mink-goutte-driver": "^1.2.1",
        "consolidation/robo": "^1.4",
        "drupal/drupal-extension": "~3.4",
        "weavora/mink-extra-context": "dev-master",
        "drupal/drupal-driver": "^1.2.1",
        "psr/log":  "*",
        "emuse/behat-html-formatter": "~0.1",
        "behat/behat":  "~3.3"
    },
    "bin": [
        "sws-caravan"
    ],
    "autoload": {
        "psr-4": {
            "StanfordCaravan\\": "src"
        },
        "psr-0": {
            "StanfordBehat\\": "src"
        }
    }
}
