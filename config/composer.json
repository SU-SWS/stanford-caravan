{
    "name": "test/test",
    "description": "CI included ",
    "type": "project",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "git@github.com:pookmish/behat-tools.git"
        }
    ],
    "require": {
        "bex/behat-screenshot": "^1.2",
        "dmore/behat-chrome-extension": "^1.0.0",
        "drupal/core": "^8.8.0",
        "drupal/drupal-extension": "^4.0",
        "jarnaiz/behat-junit-formatter": "^1.3.2",
        "mglaman/drupal-check": "^1.0",
        "php": ">=7.1",
        "squizlabs/php_codesniffer": "^3.0.1",
        "su-sws/stanford-caravan": "dev-8.x-1.x",
        "behat/mink-selenium2-driver": "^1.3.1",
        "miiimooo/behat-tools": "dev-master"
    },
    "require-dev": {
    },
    "extra": {
        "patches": {
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "custom-installer": {
            "web/libraries/{$name}/": [
                "type:component",
                "ckeditor/ckeditor",
                "enyo/dropzone",
                "components/jquery"
            ],
            "vendor/{$vendor}/{$name}/": [
                "type:library"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ]
        }
    }
}
