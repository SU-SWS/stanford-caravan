{
    "name": "test/test",
    "description": "CI included ",
    "type": "project",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.1",
        "cweagans/composer-patches": "^1.6",
        "drupal/core": "~9.4.0",
        "drush/drush": "^11.0",
        "su-sws/stanford-caravan": "dev-8.x-2.x"
    },
    "require-dev": {
        "drupal/core-dev": "^9.0",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "replace": {
        "drupal/coder": "*"
    },
    "config": {
        "php": "7",
        "sort-packages": true,
        "discard-changes": true,
        "allow-plugins": true
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patches": {},
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "custom-installer": {
            "web/libraries/{$name}/": [
                "type:component",
                "ckeditor/ckeditor",
                "enyo/dropzone",
                "components/jquery"
            ],
            "vendor/{$vendor}/{$name}/": [
                "type:library"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ]
        }
    }
}
